{
  "name": "ng-skeleton-loading",
  "version": "0.0.16",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-lib:ng-skeleton-loading": "ng build ng-skeleton-loading",
    "npm-pack:ng-skeleton-loading": "cd dist/ng-skeleton-loading && npm pack",
    "package:ng-skeleton-loading": "npm run build-lib:ng-skeleton-loading && npm run npm-pack:ng-skeleton-loading",
    "publish": "npm run package:ng-skeleton-loading && cd dist/ng-skeleton-loading && npm publish --access=public --registry https://registry.npmjs.org/",
    "unpublish": "npm unpublish ng-skeleton-loading --force --registry  https://registry.npmjs.org/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "ng-packagr": "^16.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/localize": "^16.0.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~5.0.2"
  },
  "volta": {
    "node": "20.5.0",
    "npm": "9.8.0"
  }
}