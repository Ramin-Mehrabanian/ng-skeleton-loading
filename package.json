{
  "name": "ng-skeleton-loading",
  "version": "0.0.14",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-lib:ng-skeleton-loading": "ng build ng-skeleton-loading",
    "npm-pack:ng-skeleton-loading": "cd dist/ng-skeleton-loading && npm pack",
    "package:ng-skeleton-loading": "npm run build-lib:ng-skeleton-loading && npm run npm-pack:ng-skeleton-loading",
    "publish:ng-skeleton-loading": "npm run package:ng-skeleton-loading && cd dist/ng-skeleton-loading && npm publish --access=public --registry  https://registry.npmjs.org/",
    "unpublish": "npm unpublish ng-skeleton-loading --force --registry  https://registry.npmjs.org/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.3.0",
    "@angular/common": "~14.3.0",
    "@angular/compiler": "~14.3.0",
    "@angular/core": "~14.3.0",
    "@angular/forms": "~14.3.0",
    "@angular/platform-browser": "~14.3.0",
    "@angular/platform-browser-dynamic": "~14.3.0",
    "@angular/router": "~14.3.0",
    "ng-packagr": "^15.0.3",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.0.3",
    "@angular/compiler-cli": "~14.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.8.4"
  }
}